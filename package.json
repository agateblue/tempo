    "markdown-it": "^11.0.1",
    "markdown-it-attrs": "^3.0.3",
    "pouchdb": "^7.2.2",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-find": "^7.2.2",
    "register-service-worker": "^1.7.2",
    "roboto-fontface": "*",
    "vue": "^3.2.37",
    "vue-plausible": "^1.3.1",
    "vue-router": "4",
